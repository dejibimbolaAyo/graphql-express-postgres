{
  "name": "MyApp",
  "main": "src/index.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "dependencies": {
    "apollo-server-express": "^1.3.6",
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "bcrypt": "^1.0.3",
    "body-parser": "^1.18.2",
    "express": "^4.16.2",
    "express-jwt": "^5.3.0",
    "graphql": "^0.12.3",
    "graphql-import": "^0.4.1",
    "graphql-tools": "^2.19.0",
    "jsonwebtoken": "^8.1.1",
    "knex": "^0.14.2",
    "morgan": "^1.9.0",
    "nodemon": "^1.14.11",
    "objection": "^0.9.4",
    "pg": "^7.4.1"
  },
  "scripts": {
    "prestart": "npm run -s build",
    "start": "node dist/index.js",
    "dev": "nodemon src/index.js --exec \"node -r babel-register\"",
    "clean": "rimraf dist",
    "build": "npm run clean && mkdir -p dist && babel src -s -D -d dist",
    "prettier": "./node_modules/.bin/prettier --print-width 120 --single-quote --no-semi --write \"{migrations,src}/**/*.js\"",
    "eslint": "./node_modules/.bin/eslint .",
    "migrate": "./node_modules/.bin/babel-node ./node_modules/.bin/knex migrate:latest",
    "test": "jest --watch",
    "coveralls": "jest --coverage && cat ./coverage/lcov.info | coveralls"
  },
  "devDependencies": {
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-register": "^6.26.0",
    "coveralls": "^3.0.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.10.0",
    "jest": "^22.4.2",
    "prettier": "1.10.2",
    "rimraf": "^2.6.2"
  }
}
